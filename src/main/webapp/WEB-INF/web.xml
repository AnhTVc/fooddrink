<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>AdminRestaurant</servlet-name>
		<display-name>AdminRestaurant</display-name>
		<description></description>
		<servlet-class>com.project.controller.AdminRestaurant</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Restaurant</servlet-name>
		<display-name>Restaurant</display-name>
